{
  "name": "some-utils-dom",
  "version": "1.0.0",
  "description": "",
  "repository": "https://github.com/jniac/some-utils-three",
  "scripts": {
    "postinstall": "pnpm build && node -e \"console.log('Memo: Use \\\"pnpm build\\\" to keep the dist folder sync with source after any pull.')\"",
    "dev": "tsc-watch --onSuccess \"node scripts/post-build.mjs\"",
    "build:npm": "rimraf dist-npm && tsc --outDir ./dist-npm --declarationMap false --sourceMap false && node scripts/post-build.mjs -d dist-npm",
    "build": "rimraf dist && tsc && node scripts/post-build.mjs"
  },
  "keywords": [],
  "author": "jniac",
  "license": "ISC",
  "peerDependencies": {
    "some-utils-ts": "workspace:*"
  },
  "devDependencies": {
    "commander": "^14.0.1",
    "rimraf": "^6.0.1",
    "tsc-watch": "^6.2.1",
    "typescript": "^5.5.4"
  },
  "exports": {
    "./*.css": {
      "default": "./src/*.css"
    },
    "./*": {
      "import": "./dist/*.js",
      "types": "./dist/*.d.ts",
      "default": "./dist/*.js"
    }
  },
  "files": [
    "dist",
    "src"
  ]
}